<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>P4-UTS-FAHMI</display-name>
  <filter>
    <filter-name>sitemesh</filter-name>
    <filter-class>com.opensymphony.sitemesh.webapp.SiteMeshFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>sitemesh</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>FormMahasiswaServlet</servlet-name>
    <servlet-class>com.p4uts.fahmi.servlet.FormMahasiswaServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DataMahasiswa</servlet-name>
    <servlet-class>com.p4uts.fahmi.servlet.DataMahasiswa</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FormMahasiswaServlet</servlet-name>
    <url-pattern>/FormMahasiswaServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DataMahasiswa</servlet-name>
    <url-pattern>/DataMahasiswa</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>DataMahasiswa</welcome-file>
  </welcome-file-list>
</web-app>